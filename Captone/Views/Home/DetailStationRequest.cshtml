@model IEnumerable<Captone.Models.Request>
<div>
    <table class="table">
        <tr>
            <th>Mã Yêu Cầu
            </th>
            <th>Khách Hàng
            </th>
            <th>Ngày Gửi
            </th>
        </tr>

        @foreach (var item in Model)
        {            

                    <tr>
                        <td>@item.RequestCode</td>
                        <td>@item.Username</td>
                        <td>@item.DateRequest</td>
                    </tr>      
        }
    </table>
</div>